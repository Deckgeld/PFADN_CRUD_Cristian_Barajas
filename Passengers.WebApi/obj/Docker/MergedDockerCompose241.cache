name: dockercompose1606922811225490709
services:
  mysql:
    container_name: mysql_passagers
    environment:
      MYSQL_ROOT_PASSWORD: admin
    image: mysql
    networks:
      passagersNetwork: null
    ports:
    - mode: ingress
      target: 3306
      published: "33061"
      protocol: tcp
    volumes:
    - type: bind
      source: D:/Deck/Documents/GitHub/CRUD_Cristian_Barajas/mysql/crudpassengers.sql
      target: /mysql.sql
      bind:
        create_host_path: true
    - type: volume
      source: mysql
      target: /var/lib/mysql
      volume: {}
  passengers.webapi:
    build:
      context: D:\Deck\Documents\GitHub\CRUD_Cristian_Barajas\Passengers.WebApi
      dockerfile: Passengers.WebApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Passengers.WebApi
    container_name: Passengers.WebApi
    depends_on:
      mysql:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: passengerswebapi
    labels:
      com.microsoft.visual-studio.project-name: Passengers.WebApi
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/Passengers.WebApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      passagersNetwork: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\Deck\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Deck\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Deck\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  passagersNetwork:
    name: dockercompose1606922811225490709_passagersNetwork
    driver: bridge
volumes:
  mysql:
    name: dockercompose1606922811225490709_mysql